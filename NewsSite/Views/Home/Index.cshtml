@model List<DomainModel.Models.News>
@{
    Layout = "~/Views/Shared/VisitoryLayout.cshtml";
}

<div class="container">
    <div class="row pb-5">
        <div class="col-md-6 col-12 mt-3">
            @await Component.InvokeAsync("SpecialNews")
        </div>
        <div class="col-md-6 col-12">
            <div class="row">
                <div class="col-12">
                    @await Component.InvokeAsync("LatestSpecialNews")
                </div>

            </div>
        </div>
    </div>
</div>